  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#939F91",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#8DA101"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#DF69BA"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#DF69BA"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#35A77C"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#DF69BA"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#5C6A72"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#F85552",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#F57D26"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#F57D26",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#DFA000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#8DA101",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#8DA101"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#DFA000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#F57D26"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#DFA000"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#5C6A72"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#DF69BA"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#F85552",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Special",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#8DA101"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#5C6A72"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#939F91"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#3A94C5"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#F57D26",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#5C6A72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#5C6A72",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#8DA101"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#3A94C5"
      }
    }
  ]
}