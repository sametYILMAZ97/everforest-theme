  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#859289",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#83C092"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "support.constant.property-value"],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#E67E80",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#E69875"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#E69875",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#DBBC7F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#A7C080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function call",
      "scope": ["meta.function-call", "variable.function"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#DBBC7F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E69875"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "name": "Attribute",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "name": "Preprocessor",
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#E67E80",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Special",
      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#859289"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "meta.object-literal.key"],
      "settings": {
        "foreground": "#7FBBB3"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#E69875",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#D3C6AA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#D3C6AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7FBBB3"
      }
    }
  ]
}
